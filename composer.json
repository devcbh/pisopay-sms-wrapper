{
    "name": "devcbh/sms-wrapper",
    "description": "A Laravel wrapper for sending SMS via Macrokiosk",
    "type": "library",
    "license": "proprietary",
    "version": "1.0.0",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Devcbh\\SmsWrapper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devcbh\\SmsWrapper\\SmsServiceProvider"
            ],
            "aliases": {
                "Sms": "Devcbh\\SmsWrapper\\Facades\\Sms"
            }
        }
    },
    "authors": [
        {
            "name": "Christian Villegas",
            "email": "cv@pisopay.com.ph"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^9.16",
        "phpunit/phpunit": "^12.5"
    }
}
